{
    "name": "sndatabase/pdo",
    "description": "PDO Plugin of the SN Database api",
    "type": "library",
    "keywords": ["database", "abstraction"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samy Naamani",
            "email": "samy@namani.net"
        }
    ],
    "support": {
        "email": "samy@namani.net"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "PHP": ">=5.3",
        "sndatabase/core": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.x",
        "phpunit/phpunit-skeleton-generator": "2.0.x"
    },
    "autoload": {
        "psr-4": {
            "SNDatabase\\PDO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SNDatabase\\Tests\\": "tests/"
        }
    }
}
